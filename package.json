{
  "name": "usurper-blueprints",
  "author": "Hesburgh Libraries - WSE <web-and-software-engineering-group@nd.edu> (https://library.nd.edu)",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndlib/usurper-blueprints.git"
  },
  "bugs": {
    "url": "https://github.com/ndlib/usurper-blueprints/issues"
  },
  "homepage": "https://github.com/ndlib/usurper-blueprints#readme",
  "keywords": [
    "aws",
    "cdk",
    "ndlib"
  ],
  "bin": {
    "deploy": "bin/deploy.js"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git checkout -B bump-$npm_package_version && git add -A src",
    "postversion": "git push --set-upstream origin bump-$npm_package_version && git push --tags",
    "watch": "tsc-watch --onSuccess \"npm run test\""
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.9.0",
    "@types/jest": "^24.0.18",
    "aws-cdk": "^1.9.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "tsc-watch": "^4.0.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.9.0",
    "@aws-cdk/aws-cloudfront": "^1.9.0",
    "@aws-cdk/aws-codebuild": "^1.9.0",
    "@aws-cdk/aws-codepipeline": "^1.9.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.9.0",
    "@aws-cdk/aws-iam": "^1.9.0",
    "@aws-cdk/aws-s3": "^1.9.0",
    "@aws-cdk/aws-s3-deployment": "^1.9.0",
    "@aws-cdk/aws-sns": "^1.9.0",
    "@aws-cdk/core": "^1.9.0",
    "ndlib-cdk": "^1.0.2"
  }
}
